{
	"AppPath":"/app/",
	"Port":":9999",
	"DefaultPath":"home/index",
	"NotFoundPath":"home/nofound",
	"GofSessionId": "_gofSessionId_",
	"SessionMode":"InProc",
	"EnableSession":true,
	"SessionExpires":60,
	"RoutesRule":{
		"0":"/app/{controller}/{action}/{id}",
		"1":"/app/Home/{controller}/{action}/{id}"
	},
	"HandlerSortings":{
		"github.com/justinhuang917/gof/gofcore.SessionHandler": -1,
		"github.com/justinhuang917/gof/gofcore.DefaultHandler": 0
	}
}